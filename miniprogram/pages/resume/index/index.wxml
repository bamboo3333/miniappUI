	<view class="content">
		<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
			<block wx:for="{{imgURL}}" wx:key="">
				<swiper-item>
					<image src="{{item}}" class="slide-image" />
				</swiper-item>
			</block>
		</swiper>
	</view>
	<view class="line"></view>
	<view class="resume-title">
		<view class="resume-recommend-title">
			<image src="../../../images/resume/classification.png" />
			<text class="title">简历分类</text>
		</view>
		<view class="hr"></view>
		<view class="unav">
			<view class="navlist" bindtap="gotopage">
				<view class="navthumb">
					<image src="../../../images/hire-new/trainee.png" mode="scaleToFill"></image>
				</view>
				<view class="navname">实习生</view>
			</view>
			<view class="navlist" bindtap="gotopage">
				<view class="navthumb">
					<image src="../../../images/hire-new/fullTime.png" mode="scaleToFill"></image>
				</view>
				<view class="navname">全职</view>
			</view>
			<view class="navlist" bindtap="gotopage">
				<view class="navthumb">
					<image src="../../../images/hire-new/partTimeJob.png" mode="scaleToFill"></image>
				</view>
				<view class="navname">兼职</view>
			</view>
		</view>
	</view>
	<view class="line"></view>

	<view class="container">
		<!-- 简历推荐 -->
		<!-- 根据简历使用的次数进行推荐 -->
		<view class="resume-title">
			<view class="resume-recommend-title">
				<image src="../../../images/resume/recommend.png" />
				<text class="title">简历推荐</text>
			</view>
			<view class="hr"></view>
			<view class="resume-recommend">
      <view wx:for="{{ recommend }}">
        <view class="item"  bindtap="gotoPageResume" data-index="{{ index }}">
					<image src="{{ item.modelImg }}" />
					<text class="item-text">{{ item.resumeType }}</text>
				</view>
      </view>

			</view>
		</view>
		<foot id="foot"></foot>
	</view>