	<view class="content">
		<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
			<block wx:for="{{imgURL}}" wx:key="">
				<swiper-item>
					<image src="{{item}}" class="slide-image" />
				</swiper-item>
			</block>
		</swiper>
	</view>
	<view class="line"></view>
	<view class="resume-title">
		<view class="resume-recommend-title">
			<image src="{{classificationIcon}}" />
			<text class="title">简历分类</text>
		</view>
		<view class="hr"></view>
		<view class="resume-nav">
			<view class="nav-list" bindtap="gotopage">
				<view class="nav">
					<image src="{{trainee.url}}" mode="scaleToFill"></image>
				</view>
				<view class="nav-text">{{trainee.text}}</view>
			</view>
			<view class="nav-list" bindtap="gotopage">
				<view class="nav">
					<image src="{{fullTime.url}}" mode="scaleToFill"></image>
				</view>
				<view class="nav-text">{{fullTime.text}}</view>
			</view>
			<view class="nav-list" bindtap="gotopage">
				<view class="nav">
					<image src="{{partTimeJob.url}}" mode="scaleToFill"></image>
				</view>
				<view class="nav-text">{{partTimeJob.text}}</view>
			</view>
		</view>
	</view>
	<view class="line"></view>

	<view class="container">
		<!-- 简历推荐 -->
		<!-- 根据简历使用的次数进行推荐 -->
		<view class="resume-title">
			<view class="resume-recommend-title">
				<image src="{{recommend}}" />
				<text class="title">简历推荐</text>
			</view>
			<view class="hr"></view>
			<view class="resume-recommend">
				<view class="item"  bindtap="gotoPageResume">
					<image src="https://ss1.bdstatic.com/70cFvXSh_Q1YnxGkpoWK1HF6hhy/it/u=2857793315,1593156858&fm=26&gp=0.jpg" />
					<text class="item-text">软件工程师</text>
				</view>
				<view class="item"  bindtap="gotoPageResume">
					<image src="https://ss3.bdstatic.com/70cFv8Sh_Q1YnxGkpoWK1HF6hhy/it/u=1829900109,3973718518&fm=26&gp=0.jpg" />
					<text class="item-text">AI工程师</text>
				</view>
				<view class="item"  bindtap="gotoPageResume">
					<image src="https://t9.baidu.com/it/u=2305251038,873310253&fm=193" />
					<text class="item-text">算法工程师</text>
				</view>
			</view>
		</view>
		<foot id="foot"></foot>
	</view>